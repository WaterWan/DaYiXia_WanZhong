package music;

import java.applet.Applet;
import java.applet.AudioClip;
import java.io.File;
import java.net.URL;

public class Music {
	

	public static void main(String[] args) {
		File file = new File("E:\大一下\上传\DaYiXia_WanZhong\俄罗斯方块\DEMO\bgm\Battle001.wav");
		URL url;
		try {
			url = file.toURL();
			AudioClip clip = Applet.nowAudioClip(url);
			clip.play();
		} catch (Exception e) {
			e.printStackTrace();
		}
	}

}
