package homework_2016_3_1;

import java.util.Scanner;

public class Homework4 {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int sum = 0;
		Homework3 h = new Homework3();
		for (int i = 1; i <= n; i++) {
			sum += h.square(i);
		}
		System.out.println("1到n的平方和为" + sum);
	}
}
