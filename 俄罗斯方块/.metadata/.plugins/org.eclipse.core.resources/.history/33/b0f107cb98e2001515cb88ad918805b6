package calculator;

import java.awt.Frame;

import javax.swing.JFrame;

public class JFrameCalculator extends Frame{
	public JFrameCalculator(){
		setTitle("JavaCalculator");
		setSize(500, 600);
		setVisible(true);
		JFrame.setDefaultLookAndFeelDecorated(true);
	}
	public static void main(String[] args) {
		JFrameCalculator jfc = new JFrameCalculator();
	}
}
